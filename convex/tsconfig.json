{
  "compilerOptions": {
    "jsx": "react-jsx",
    "paths": {
      "convex/*": ["./convex/*"]
    },
    "module": "commonjs",
    "target": "es2021",
    "lib": ["es2021"],
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["."]
}
